{
  "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "custom": false,
      "palette": [
        {
          "slug": "black",
          "color": "#353535",
          "name": "Black"
        },
        {
          "slug": "gray",
          "color": "#6b6b6b",
          "name": "Gray"
        }
      ]
    },
    "layout": {
      "contentSize": "750px",
      "wideSize": "750px"
    },
    "spacing": {
      "units": ["px","em","rem"]
    },
    "typography": {
      "defaultFontSizes": false,
      "dropCap": false,
      "fontSizes": [
			  {
				  "slug": "small",
				  "size": "0.8rem",
				  "name": "Small"
			  },
        {
          "slug": "medium",
          "size": "1rem",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "1.3rem",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "1.5rem",
          "name": "Extra Large"
        },
        {
          "slug": "xx-large",
          "size": "1.7rem",
          "name": "Extra Extra Large"
        }
		  ],
      "fontFamilies": [
        {
          "name": "JetBrains Mono",
					"slug": "jetbrains-mono",
					"fontFamily": "'JetBrains Mono', monospace",
          "fontFace": [
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "100",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-Thin.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "100",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-ThinItalic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "200",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-ExtraLight.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "200",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-ExtraLightItalic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "300",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-Light.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "300",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-LightItalic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-Regular.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "400",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-Italic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "500",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-Medium.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "500",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-MediumItalic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "600",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-SemiBold.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "600",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-SemiBoldItalic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-Bold.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "700",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-BoldItalic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "800",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-ExtraBold.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "800",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-ExtraBoldItalic.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "900",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-ExtraBold.woff2" ]
            },
            {
              "fontFamily": "JetBrains Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "900",
              "src": [ "file:./assets/fonts/JetBrainsMono/JetBrainsMono-ExtraBoldItalic.woff2" ]
            }
          ]
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--jetbrains-mono)"
    },
    "blocks": {
      "core/heading": {
        "typography": {
          "fontWeight": "700"
        }
      },
      "core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
                "fontWeight": "700",
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
      "core/post-content": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--black)"
            }
          }
        },
        "typography": {
					"lineHeight": "1.8"
				}
      },
      "core/post-date": {
        "color": {
          "text": "var(--wp--preset--color--gray)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontStyle": "italic",
          "fontWeight": "300"
        }
      },
      "core/post-excerpt": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "spacing": {
          "margin": {
            "bottom": "0",
            "top": "0"
          }
        },
        "typography": {
					"lineHeight": "1.6"
				}
      },
      "core/post-terms": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--black)"
            },
            ":hover": {
							"typography": {
                "fontWeight": "600"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/post-title": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "elements": {
					"link": {
            "color": {
              "text": "var(--wp--preset--color--black)"
            },
						"typography": {
							"textDecoration": "underline"
						}
					}
				},
        "typography": {
          "fontWeight": "700",
          "lineHeight": "1.6"
        }
      },
      "core/query-title": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontWeight": "700",
          "lineHeight": "1.6"
        }
      },
      "core/site-tagline": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        }
      },
      "core/site-title": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontSize": "2rem",
          "fontWeight": "700"
        }
      }
    },
    "elements": {
      "caption": {
        "color": {
          "text": "var(--wp--preset--color--gray)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontStyle": "italic",
          "fontWeight": "300"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      }
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ]
}